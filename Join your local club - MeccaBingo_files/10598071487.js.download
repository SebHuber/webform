      m430Index=0;
       $('.m430_exp .scroll-left-btn').hide(); 
    }
    if(m430Index == 1) $('.m430_exp .scroll-left-btn').show();
    currPos = $('.m430_container').position().left;
    scrollPosition = slides.eq(m430Index).position().left;
    scrollOffset = scrollPosition + currPos;
    
    diff = containerWidth - (m430ContainerWidth - scrollPosition);
    
    //console.log("diff =", diff);
    //console.log("scrollPosition=", scrollPosition);
    //console.log("m430ContainerWidth - scrollPosition=", m430ContainerWidth - scrollPosition);
    //console.log("containerWidth=", containerWidth);
    //console.log("m430ContainerWidth=", m430ContainerWidth);
    $('.m430_exp .scroll-right-btn').show(); 
    
    if (-diff < containerWidth){
      //console.log("Smaller");
      scrollPosition -= diff;  
      $('.m430_exp .scroll-right-btn').hide(); 
      m430Index ++;
    }    
   
    // scroll to that index
      $('.m430_container').animate(
      	{'left' : -scrollPosition}, 600 
      );
    
  });
  
  
  
