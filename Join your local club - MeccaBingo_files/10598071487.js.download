    if(m430Index==0) {
      $('.m430_exp .scroll-left-btn').hide(); 
    }
    if(m430Index <= 0)  m430Index=0;
    
    
    currPos = $('.m430_container').position().left;
    newPos = slides.eq(m430Index).position().left;
    
    $('.m430_container').animate(
      	{'left' : -newPos}, 600 
    ); 
   
    
  });
  }// end of sliderFunc 
  
  
});
