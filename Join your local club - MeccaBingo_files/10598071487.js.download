window.getCookie = function(name){
    var cookarr = document.cookie.split(/\s*;\s*/);
    for (var i = 0; i < cookarr.length; i++) {
        var c = cookarr[i].split('=');
        if (c[0] === name) return decodeURIComponent( c[1] );
    }
};



