  function sliderFunc(){
  	// starting index
  var slides = $('.m424GameTile');
  var sliderContainer = $('.m424_container');
  var m424Index = 0;
  var scrollPosition;
  var scrollOffset;
  var containerWidth;
  var m424ContainerWidth;
  var currPos;
  var newPos;
  var diff;
  

  // click listener
  $('.m424_exp .scroll-right-btn').on('click',function(){    
    
    containerWidth = $(".m424_exp .slider-container").width();
    m424ContainerWidth = ($(".m424GameTile").width() * slides.length) + (slides.length * $(".m424GameTile").eq(0).css('padding-right').slice(0,2) );
  
    // find next index
    // i + 1 or 0 if end of slides
    m424Index = ++m424Index % slides.length;
    //console.log("m424Index =", m424Index);
    if(m424Index>= slides.length-2) {
      m424Index=0;
       $('.m424_exp .scroll-left-btn').hide(); 
    }
    if(m424Index == 1) $('.m424_exp .scroll-left-btn').show();
    currPos = $('.m424_container').position().left;
    scrollPosition = slides.eq(m424Index).position().left;
    scrollOffset = scrollPosition + currPos;
    
    diff = containerWidth - (m424ContainerWidth - scrollPosition);
    
    //console.log("diff =", diff);
    //console.log("scrollPosition=", scrollPosition);
    //console.log("m424ContainerWidth - scrollPosition=", m424ContainerWidth - scrollPosition);
    //console.log("containerWidth=", containerWidth);
    //console.log("m424ContainerWidth=", m424ContainerWidth);
    $('.m424_exp .scroll-right-btn').show(); 
    
    if (-diff < containerWidth){
      //console.log("Smaller");
      scrollPosition -= diff;  
      $('.m424_exp .scroll-right-btn').hide(); 
      m424Index ++;
    }    
   
    // scroll to that index
      $('.m424_container').animate(
      	{'left' : -scrollPosition}, 600 
      );
    
  });
  
  
  
  $('.m424_exp .scroll-left-btn').on('click',function(){
    containerWidth = $(".m424_exp .slider-container").width();
