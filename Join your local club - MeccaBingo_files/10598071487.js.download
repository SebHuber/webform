  function sliderFunc(){
  	// starting index
  var slides = $('.m430GameTile');
  var sliderContainer = $('.m430_container');
  var m430Index = 0;
  var scrollPosition;
  var scrollOffset;
  var containerWidth;
  var m430ContainerWidth;
  var currPos;
  var newPos;
  var diff;
  

  // click listener
  $('.m430_exp .scroll-right-btn').on('click',function(){    
    
    containerWidth = $(".m430_exp .slider-container").width();
    m430ContainerWidth = ($(".m430GameTile").width() * slides.length) + (slides.length * $(".m430GameTile").eq(0).css('padding-right').slice(0,2) );
  
    // find next index
    // i + 1 or 0 if end of slides
    m430Index = ++m430Index % slides.length;
    //console.log("m430Index =", m430Index);
    if(m430Index>= slides.length-2) {
      m430Index=0;
       $('.m430_exp .scroll-left-btn').hide(); 
    }
    if(m430Index == 1) $('.m430_exp .scroll-left-btn').show();
    currPos = $('.m430_container').position().left;
    scrollPosition = slides.eq(m430Index).position().left;
    scrollOffset = scrollPosition + currPos;
    
    diff = containerWidth - (m430ContainerWidth - scrollPosition);
    
    //console.log("diff =", diff);
    //console.log("scrollPosition=", scrollPosition);
    //console.log("m430ContainerWidth - scrollPosition=", m430ContainerWidth - scrollPosition);
    //console.log("containerWidth=", containerWidth);
    //console.log("m430ContainerWidth=", m430ContainerWidth);
    $('.m430_exp .scroll-right-btn').show(); 
    
    if (-diff < containerWidth){
      //console.log("Smaller");
      scrollPosition -= diff;  
      $('.m430_exp .scroll-right-btn').hide(); 
      m430Index ++;
    }    
   
    // scroll to that index
      $('.m430_container').animate(
      	{'left' : -scrollPosition}, 600 
      );
    
  });
  
  
  
  $('.m430_exp .scroll-left-btn').on('click',function(){
    containerWidth = $(".m430_exp .slider-container").width();
    m430ContainerWidth = ($(".m430GameTile").width() * slides.length) + (slides.length * $(".m430GameTile").eq(0).css('padding-right').slice(0,2) );
    
    $('.m430_exp .scroll-right-btn').show(); 
    // find next index
    m430Index = --m430Index % slides.length;    
    //console.log("m430Index: ", m430Index);
    // scroll to that index
    if(m430Index==0) {
      $('.m430_exp .scroll-left-btn').hide(); 
    }
    if(m430Index <= 0)  m430Index=0;
    
    
    currPos = $('.m430_container').position().left;
    newPos = slides.eq(m430Index).position().left;
    
    $('.m430_container').animate(
      	{'left' : -newPos}, 600 
    ); 
   
    
  });
  }// end of sliderFunc 
  
  
});
